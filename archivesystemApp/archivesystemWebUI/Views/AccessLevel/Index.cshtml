

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <div class="content container-fluid">

        
        <div class="page-header">
            <div class="row">
                <div class="col-sm-12">
                    <h3 class="page-title">Access Level Management Dashboard</h3>
                    <ul class="breadcrumb">
                        <li class="breadcrumb-item active">Dashboard</li>
                    </ul>
                </div>
            </div>
        </div>


        <div class="row">
            <div class="col-md-6 col-sm-6 col-lg-6 col-xl-6">
                <a href="@Url.Action("ManageAccessLevel", "AccessLevel")">

                    <div class="card dash-widget">
                        <div class="card-body btn shadow">
                            <span class="dash-widget-icon"><i class="fa fa-cubes"></i></span>
                            <div class="dash-widget-info">
                                <h3>Manage Access Level</h3>

                            </div>
                        </div>
                    </div>
            </div>

                
          <div class="col-md-6 col-sm-6 col-lg-6 col-xl-6" >
            <a href="@Url.Action("ManageUserAccess", "UserAccess")">
                    <div class="card dash-widget" >
                        <div class="card-body btn shadow">
                            <span class="dash-widget-icon"><i class="fa fa-users"></i></span>
                            <div class="dash-widget-info">
                                <h3 >Manage User Access</h3>       
                            </div>
                        </div>
                    </div>
            </a>
          </div>
        </div>


    </div>